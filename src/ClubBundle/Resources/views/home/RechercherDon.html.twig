{% extends 'base2.html.twig' %}
{% block content %}
    <div class="page-content-wrapper">
        <div class="page-content">
            <div class="page-bar">
                <div class="page-title-breadcrumb">
                    <div class=" pull-left">
                        <div class="page-title">Rechercher Don</div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-12">
                    <div class="card-box">
                        <div class="card-head">
                            <header>Rechercher Don</header>
                        </div>
                        <div class="card-body row">
                            <h3>Formulaire de recherche d'annonce</h3>

                            <form method="post" {{ form_enctype(form) }}>

                                {{ form_widget(form) }}

                                <input type="submit" class="btn btn-primary" />

                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

{% endblock %}